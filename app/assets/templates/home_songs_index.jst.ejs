<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">Top Songs</h3>
	</div>
	<table class="table">
		<tbody>
			<% songs.each(function(song) { %>
				<tr>
					<td><a href="#/songs/<%= song.id %>"> <%= song.get('title') %>
					                <span class="badge"><%= song.taggings().models.length %></span></td>
				</tr>
			<% }) %>
		</tbody>
	</table>
</div>