<h1 class="title">All Tags</h1>

<div class="panel panel-default">
	<table class="table">
		
		<thead>
			<tr>
				<th>Name</th>
				<th>Top Segment</th>
				<th>Top Song</th>
			</tr>
		</thead>
		
		<tbody>
			<% tags.each(function(tag) { %>
				<tr>
					<td><a href="#/tags/<%= tag.id %>"><%= tag.get('name') %></td>
					<td>Not Implemented</td>
					<td>Not Implemented</td>
				</tr>
			<% }) %>
		</tbody>
		
	</table>
</div>